<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activity_signup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Activity_signup">
        <activity android:name=".Activity_Home"></activity>
        <activity android:name=".Activity_Trainer" />
        <activity android:name=".Activity_Notice" />
        <activity android:name=".Activity_workout" />
        <activity android:name=".Activity_Package" />
        <activity android:name=".Activity_userDashboard" />
        <activity android:name=".Activity_Profile" />
        <activity android:name=".activity_Bmi_Calc" />
        <activity android:name=".activity_register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>